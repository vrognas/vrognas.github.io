  author = {Timsit, Jean-Fran{\c c}ois and {de Kraker}, Marlieke E. A. and Weiss, Emmanuel and Bettiol, Esther and Wolkewitz, Martin and Nikolakopoulos, Stavros and Wilson, David and Harbarth, Stephan},
